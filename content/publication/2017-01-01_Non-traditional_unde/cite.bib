@Article{sanchez-gelabert_non-traditional_2017,
  title = {Los estudiantes universitarios no tradicionales y el abandono de los estudios},
  author = {Albert Sánchez-Gelabert and Marina Elias},
  year = {2017},
  doi = {10.15581/004.32.27-48},
  volume = {32},
  issn = {1578-7001},
  abstract = {En el presente artículo se propone una tipología de estudiantes universitarios no tradicionales a partir de la identiﬁ cación de las características más relevantes en nuestro contexto y se analiza su impacto en el abandono de los estudios teniendo en cuenta el rendimiento académico. A partir del análisis de una cohorte de nuevo acceso (n=6367) en la Universidad Autónoma de Barcelona, se realizan análisis mediante el test de chi-cuadrado de Pearson y los residuos tipiﬁcados corregidos. Los resultados muestran que los estudiantes no tradicionales tienden a abandonar con más frecuencia que sus compañeros tradicionales aun teniendo un buen rendimiento.},
  journal = {Estudios Sobre Educación},
  keywords = {Academic achievement, Higher education dropout, Higher education participation, Non-traditional students},
  pubtype = {2},
  date = {2017-01-01},
}
